{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node start.js",
    "server": "nodemon start.js",
    "client": "cd client && yarn start",
    "dev": "concurrently \"yarn run server\" \"yarn run client\"",
    "heroku-postbuild":
      "yarn cache clean && cd client && yarn install --production=false && yarn build"
  },
  "engines": {
    "node": "8.6.x",
    "npm": "5.6.x",
    "yarn": "1.3.2"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "body-parser": "~1.13.2",
    "concurrently": "^3.5.1",
    "cookie-parser": "~1.3.5",
    "cross-env": "^5.1.5",
    "express": "~4.13.1",
    "express-async-errors": "^2.1.2",
    "jsonwebtoken": "^8.1.0",
    "md5": "^2.2.1",
    "mongoose": "^4.13.7",
    "morgan-body": "^2.0.2",
    "nodemon": "^1.13.3",
    "react-loadable": "^5.3.1",
    "react-loadable-visibility": "^2.4.2",
    "react-simple-popover": "^0.2.4",
    "repl-promised": "^0.1.0",
    "validator": "^9.2.0"
  }
}
